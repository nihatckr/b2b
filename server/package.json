{
  "name": "@fullstack/server",
  "version": "1.0.0",
  "description": "GraphQL API server with Prisma and Apollo",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "npm run clean && npm run generate && tsc",
    "clean": "rimraf dist",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema.ts",
    "seed": "ts-node prisma/seed.ts",
    "seed:users-only": "ts-node prisma/seed-users-only.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "echo \"No tests yet\" && exit 0",
    "fix-notification-links": "ts-node src/utils/fixNotificationLinks.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "graphql",
    "prisma",
    "apollo",
    "typescript",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/graphql-upload": "^17.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.1",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.17.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@prisma/client": "^6.17.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "graphql": "^16.11.0",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.25.0",
    "graphql-shield": "^7.6.5",
    "graphql-upload": "^17.0.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "nexus": "^1.3.0",
    "sharp": "^0.34.4",
    "uuid": "^13.0.0"
  }
}
